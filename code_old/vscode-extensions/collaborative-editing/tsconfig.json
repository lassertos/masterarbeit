{
	"compilerOptions": {
		"module": "ES2020",
		"target": "ES2020",
		"outDir": "dist",
		"moduleResolution": "Bundler",
		"lib": [
			"ES2020", "WebWorker", "DOM"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true
	}
}
