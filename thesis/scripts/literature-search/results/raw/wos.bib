Keywords = {data pipeline; extract-transform-load; database; data aggregation; data
   processing; plant phenotyping},
Author-Email = {drwang@purdue.edu},
ORCID-Numbers = {Vargas Rojas, Luis/0000-0001-8610-9901},
Times-Cited = {0},
Unique-ID = {WOS:001176021400001},
DA = {2024-08-03},
}

