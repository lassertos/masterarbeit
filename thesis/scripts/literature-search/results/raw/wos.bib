@article{ WOS:000316878000009,
Author = {Buter, R. K. and Van Raan, A. F. J.},
Title = {Identification and analysis of the highly cited knowledge base of
   sustainability science},
Journal = {SUSTAINABILITY SCIENCE},
Year = {2013},
Volume = {8},
Number = {2},
Pages = {253-267},
Month = {APR},
Abstract = {We investigated the interdisciplinary `pillars' of scientific knowledge
   on which the emerging field of sustainability science is founded, using
   a bibliometric approach and data from the Web of Science database. To
   find this scientific basis, we first located publications that represent
   a relevant part of sustainability science and then extracted the set of
   best cited publications, which we called the highly cited knowledge base
   (HCKB). To find the research orientation in this set, we inspected the
   occurrence of fields and contrasted this with the occurrence of fields
   in other publication sets relevant to sustainability science. We also
   created a network of co-cited HCKB publications using the seed set
   citations, extracted communities or clusters in this network and
   visualised the result. Additionally, we inspected the most cited
   publications in these HCKB clusters. We found that themes related to the
   three pillars of sustainable development (environment, economy and
   sociology) are all present in the HCKB, although social science (not
   including economics) is less visible. Finally, we found increasing
   diversity of fields and clusters in the citations of the seed set,
   indicating that the field of sustainability science is not yet moving
   into a more transdisciplinary state.},
Type = {Article},
Affiliation = {Buter, RK (Corresponding Author), Leiden Univ, Ctr Sci \& Technol Studies, POB 905, NL-2300 AX Leiden, Netherlands.
