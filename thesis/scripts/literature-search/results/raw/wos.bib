@article{ WOS:000526767200001,
Author = {Tucker, Tyler and Giglio, Donata and Scanderbeg, Megan and Shen, Samuel
   S. P.},
Title = {Argovis: A Web Application for Fast Delivery, Visualization, and
   Analysis of Argo Data},
