@article{ WOS:000287900100009,
Author = {Chen, C. H. and Lin, H. C.},
Title = {A Distance e-Learning Platform for Signal Analysis and Measurement Using
   FFT},
Journal = {COMPUTER APPLICATIONS IN ENGINEERING EDUCATION},
Year = {2011},
Volume = {19},
Number = {1},
Pages = {71-80},
Month = {MAR},
Abstract = {The fast Fourier transform (FFT) and the power spectrum are still
   powerful tools for analyzing and measuring both stationary and transient
   signals in power systems. However, the misapplications of FFT can lead
   to incorrect results caused by some problems such as aliasing effect,
   spectral leakage, and picket-fence effect. Measurement errors can be
   efficiently reduced by understanding fundamentals of FFT as well as
   loading proper windows. This article develops a distance e-learning
   environment using a graphical programming tool to help electrical
