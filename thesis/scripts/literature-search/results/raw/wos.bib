Keywords = {Graph-based service modelling; TOSCA; Orchestration; Time-critical cloud
   application; Co-programming; Quality of service},
Author-Email = {StefanicP@cardiff.ac.uk},
ORCID-Numbers = {Knight, Louise/0000-0003-1431-197X},
Times-Cited = {5},
Unique-ID = {WOS:000501935700071},
