@article{ WOS:000229614200009,
Author = {Darling, J and Sauvage, A},
Title = {The application environment},
Journal = {BT TECHNOLOGY JOURNAL},
Year = {2005},
Volume = {23},
Number = {1},
Pages = {82-89},
Month = {JAN},
Abstract = {There is consensus within the communications industry that the design
   pattern for the intelligence architecture will be based on a
   componentisation model where the richness of the underlying network and
   services will be available as capabilities. This approach is aimed at
   maximising reuse of capabilities for next generation applications and
   supporting the diverse range of business models. To make this model
   work, an application environment is proposed that exposes the
   capabilities to the service provider (and their application developers)
   and supports them through the complete application life cycle from
   engagement, development, deployment, execution and withdrawal.
   The architecture for the application environment consists of four major
   components - capability exposure function, application hosting, the
   service provider framework and the service subscription function. This
   architecture can be implemented using technologies such as Web Services
   (including UDDI), open standard interfaces such as Parlay and SIP
   servlets, and development environments such as J2EE and .NET.},
Type = {Article},
Affiliation = {Darling, J (Corresponding Author), BT, 21CN Project, Applicat Architecture, Ipswich, Suffolk, England.
