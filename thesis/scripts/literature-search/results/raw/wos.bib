Keywords = {Pure-feedback; time-derivative estimator; approximation-free; uncertain
   nonlinear system},
Keywords-Plus = {ADAPTIVE NEURAL-CONTROL; DYNAMIC SURFACE CONTROL; SLIDING MODE CONTROL;
