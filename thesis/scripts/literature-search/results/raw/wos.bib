Author = {Li, Yinsheng and Shen, Jianping and Shi, Junshuai and Shen, Weiming and
   Huang, Ying and Xu, Yingxiao},
Title = {Multi-model driven collaborative development platform for
   service-oriented e-Business systems},
Journal = {ADVANCED ENGINEERING INFORMATICS},
Year = {2008},
Volume = {22},
Number = {3},
Pages = {328-339},
