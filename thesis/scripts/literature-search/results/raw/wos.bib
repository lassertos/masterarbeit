@article{ WOS:000322422700004,
Author = {Lieveld, Faydra I. and van Vlerken, Lotte G. and Siersenna, Peter D. and
   van Erpecum, Karel J.},
Title = {Patient adherence to antiviral treatment for chronic hepatitis B and C:
   a systematic review},
Journal = {ANNALS OF HEPATOLOGY},
Year = {2013},
Volume = {12},
Number = {3},
Pages = {380-391},
Month = {MAY-JUN},
Abstract = {Introduction. Poor adherence to treatment for various chronic diseases
   is a frequent phenomenon. Current guidelines for the treatment of
   chronic hepatitis B (HBV) and hepatitis C (HCV) recommend optimal
   adherence, since it has been suggested that poor adherence is associated
   with an increased risk of virological failure. We aimed to give an
   overview of studies exploring adherence to combination treatment
   (PEG-interferon plus ribavirin) for HCV and nucleos(t)ide analogues for
   HBV. Material and methods. A systematic review Was conducted using the
   databases PubMed, Embase, Cochrane Library and Web of Knowledge. Search
   terms included ``adherence{''} or ``compliance{''} combined with
   ``hepatitis B{''}, ``hepatitis C{''} or ``viral hepatitis{''}. Results.
   The final selection included 19 studies (13 HCV, 6 HBV). Large
   differences in patient numbers and adherence assessment methods were
   found between the various studies. For HCV mean adherence varied from 27
   to 97\%, whereas the proportion of patients with >= 80\% adherence
   varied from 27 to 96\%. Mean adherence reported in HBV studies ranged
   from 81 to 99\%, with 66 to 92\% of patients being 100\% adherent. For
   both HCV and HBV studies, the highest adherence rates were reported in
   studies using self-report whereas lower adherence rates were reported in
   studies using pharmacy claims. Poor adherence to treatment was
   associated with an increased risk of virological failure. Conclusion.
   Non-adherence to treatment in chronic viral hepatitis is not a frequent
   phenomenon. However, given the increased risk of virological failure in
   poorly adherent patients, clinicians should routinely address adherence
   issues in all patients treated for chronic viral hepatitis.},
Type = {Review},
Affiliation = {van Erpecum, KJ (Corresponding Author), Univ Med Ctr Utrecht, Dept Gastroenterol \& Hepatol, F-02-618,POB 85500, NL-3508 GA Utrecht, Netherlands.
   Lieveld, Faydra I.; van Vlerken, Lotte G.; Siersenna, Peter D.; van Erpecum, Karel J., Univ Med Ctr Utrecht, Dept Gastroenterol \& Hepatol, NL-3508 GA Utrecht, Netherlands.},
DOI = {10.1016/S1665-2681(19)31000-2},
ISSN = {1665-2681},
Keywords = {Compliance; Nucleos(t)ide analogues; Peg-interferon; Ribavirin; Viral
   hepatitis},
Keywords-Plus = {QUALITY-OF-LIFE; MEDICATION ADHERENCE; REPORTED ADHERENCE; THERAPY;
   PERSISTENCE; PROGRESSION; INFECTION; OUTCOMES},
Author-Email = {K.J.vanErpecum@umcutrecht.nl},
Times-Cited = {54},
Unique-ID = {WOS:000322422700004},
DA = {2024-08-03},
}

@article{ WOS:000310171300015,
