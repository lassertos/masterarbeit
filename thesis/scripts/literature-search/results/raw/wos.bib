   Zinkiewicz, Daniel, WASAT Sp Zoo, Trzy Lipy 3, PL-80172 Gdansk, Poland.},
DOI = {10.3390/rs11171973},
Article-Number = {1973},
EISSN = {2072-4292},
Keywords = {Docker; eEarth observation data processing; IDE; IPython; Jupyter
   notebook; web application; web processing service},
