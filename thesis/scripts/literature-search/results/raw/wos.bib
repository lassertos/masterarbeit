Affiliation = {Oda, T (Corresponding Author), Software Res Assoc Inc, Key Technol Lab, Tokyo 1718513, Japan.
   Oda, Tomohiro, Software Res Assoc Inc, Key Technol Lab, Tokyo 1718513, Japan.
   Araki, Keijiro, Kyushu Univ, Dept Adv Informat Technol, Fukuoka 8190395, Japan.
   Larsen, Peter Gorm, Aarhus Univ, Dept Engn, Aarhus, Denmark.},
DOI = {10.1587/transinf.2016FOP0003},
ISSN = {1745-1361},
Keywords = {lightweight formal methods; formal specification; VDM; integrated
   development environment},
Keywords-Plus = {VDM},
