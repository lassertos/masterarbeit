@article{ WOS:000173533600006,
Author = {Carmichael, P and Honour, L},
Title = {Open Source as appropriate technology for global education},
Journal = {INTERNATIONAL JOURNAL OF EDUCATIONAL DEVELOPMENT},
Year = {2002},
Volume = {22},
Number = {1},
Pages = {47-53},
Month = {JAN},
Abstract = {Economic arguments for the adoption of `open source' software in
   business have been widely discussed. In this paper we draw on personal
   experience in the UK, South Africa and Southeast Asia to forward
   compelling reasons why open source software should be considered as an
   appropriate and affordable alternative to the currently prevailing
   dependency on large commercial organisations and proprietary products in
   the field of education.
   The dynamic and responsive nature of `open source' software and the
   existence of freely available documentation and online communities
   offers an opportunity for educators, network administrators and software
   developers to participate in the development of resources appropriate to
   local needs while developing their own skills.
   We identify a range of critical development tools such as Perl and
   Linux, alongside a more specific application, Basic Support for
   Cooperative Work, which has great versatility for customising to fulfil
   specific educational needs and for the development of collaborative
   on-line learning communities. (C) 2002 Elsevier Science Ltd. All rights
   reserved.},
