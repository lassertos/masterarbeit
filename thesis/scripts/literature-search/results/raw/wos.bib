@article{ WOS:000901585700005,
Author = {Abekiri, Najib and Rachdy, Azzedine and Ajaamoum, Mohammed and Nassiri,
   Boujemaa and Elmahni, Lahoussine and Oubail, Youssef},
Title = {Platform for hands-on remote labs based on the ESP32 and NOD-red},
Journal = {SCIENTIFIC AFRICAN},
Year = {2023},
Volume = {19},
Month = {MAR},
Abstract = {Not only in Morocco, throughout the walks of the world covid 19
   pandemics has seriously questioned policymakers from different sectors.
   Think-tank in the educational sector notably higher education addressed
   by such a wide range of challenges brought about by covid 19. The
   characteristic concern that educationalists in Moroccan universities
   have to reconsider in this pandemic period should not be beyond
   rethinking new pedagogical alternatives including approaches, methods,
   techniques and didactic materials which can successfully assist
