Title = {Integrated document browsing and data acquisition for building large
   ontologies},
Booktitle = {KNOWLEDGE-BASED INTELLIGENT INFORMATION AND ENGINEERING SYSTEMS, PT 3,
   PROCEEDINGS},
Series = {Lecture Notes in Artificial Intelligence},
Year = {2006},
Volume = {4253},
