Keywords = {FPGA-IP; eFPGA; FPGA CAD; FPGA architecture; application-specific
   acceleration},
Keywords-Plus = {FRAMEWORK},
Author-Email = {kuga@cs.kumamoto-u.ac.jp},
Times-Cited = {1},
Unique-ID = {WOS:000944643400006},
DA = {2024-08-03},
}

@article{ WOS:000374968300001,
Author = {Vuorimaa, Petri and Laine, Markku and Litvinova, Evgenia and Shestakov,
   Denis},
Title = {Leveraging declarative languages in web application development},
Journal = {WORLD WIDE WEB-INTERNET AND WEB INFORMATION SYSTEMS},
Year = {2016},
Volume = {19},
Number = {4},
Pages = {519-543},
Month = {JUL},
Abstract = {Web Applications have become an omnipresent part of our daily lives.
   They are easy to use, but hard to develop. WYSIWYG editors, form
   builders, mashup editors, and markup authoring tools ease the
   development of Web Applications. However, more advanced Web Applications
   require servers-side programming, which is beyond the skills of end-user
   developers. In this paper, we discuss how declarative languages can
   simplify Web Application development and empower end-users as Web
   developers. We first identify nine end-user Web Application development
   levels ranging from simple visual customization to advanced three-tier
   programming. Then, we propose expanding the presentation tier to support
   all aspects of Web Application development. We introduce a unified
   XForms-based framework-called XFormsDB-that supports both client-side
   and server-side Web Application development. Furthermore, we make a
   language extension proposal-called XFormsRTC-for adding true real-time
   communication capabilities to XForms. We also present XFormsDB
   Integrated Development Environment (XIDE), which assists end-users in
   authoring highly interactive data-driven Web Applications. XIDE supports
   all Web Application development levels and, especially, promotes the
   transition from markup authoring and snippet programming to single and
   unified language programming.},
Type = {Article},
Affiliation = {Vuorimaa, P (Corresponding Author), Aalto Univ, Dept Comp Sci, FI-00076 Aalto, Finland.
   Vuorimaa, Petri; Laine, Markku; Litvinova, Evgenia; Shestakov, Denis, Aalto Univ, Dept Comp Sci, FI-00076 Aalto, Finland.},
