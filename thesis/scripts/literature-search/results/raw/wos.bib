@article{ WOS:000302617000003,
Author = {Aispuro, Elvia E. and Licea, Guillermo and Suarez, Jaime and Sandoval,
   Andres and Carreno, Monica A. and Estrada, Italia and Juarez-Ramirez,
   Reyes and Aguilar, Leocundo and Martinez, Luis G.},
Title = {Supporting the Development of Interactive Applications in Introductory
