Journal = {COMPUTER APPLICATIONS IN ENGINEERING EDUCATION},
Year = {2012},
Volume = {20},
Number = {2},
Pages = {214-220},
Month = {JUN},
Abstract = {Extended Library for Visual Interactive Applications (ELVIA) is a
   programming tool developed and used by the two most important public
   universities in the Baja California peninsula in Mexico. ELVIA provides
