@article{ WOS:000329405800004,
Author = {Teixeira, Claudio and Pinto, Joaquim Sousa and Azevedo, Ricardo and
   Batista, Tiago and Monteiro, Andre},
Title = {The Building Blocks of a PaaS},
Journal = {JOURNAL OF NETWORK AND SYSTEMS MANAGEMENT},
Year = {2014},
Volume = {22},
Number = {1},
Pages = {75-99},
Month = {JAN},
Abstract = {Traditional cloud computing providers enable developers to program and
   deploy applications in the cloud by using platform as a service (PaaS)
   providers. Despite the benefits of such an approach, this service model
   usually comes with a high level of the lock in effect into the service
   provider. The lock in effect occurs when a software developer needs to
   change its cloud provider. When this happens, it usually means a major
   application rewrite, specific for the new PaaS. This paper details the
   initial results of a project whose goal is to build a PaaS where vendor
   lock in does not occur. With this PaaS, developers wishing to deploy
   their applications into the cloud may continue to program using their
   usual development environments. There are no necessary changes required
   to make the application PaaS compatible. As a proof of concept, we
   developed an open source PaaS management application as a regular web
   application and then deployed it on the cloud.},
Type = {Article},
Affiliation = {Teixeira, C (Corresponding Author), Univ Aveiro, DETI, Campus Univ Santiago, P-3810193 Aveiro, Portugal.
   Teixeira, Claudio; Pinto, Joaquim Sousa; Batista, Tiago; Monteiro, Andre, Univ Aveiro, DETI, P-3810193 Aveiro, Portugal.
