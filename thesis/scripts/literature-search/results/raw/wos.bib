@article{ WOS:000775455200001,
Author = {Damisa, Uyikumhe and Nwulu, I, Nnamdi},
Title = {Blockchain-Based Auctioning for Energy Storage Sharing in a Smart
   Community},
Journal = {ENERGIES},
Year = {2022},
Volume = {15},
Number = {6},
Month = {MAR},
Abstract = {The increasing prevalence of renewable energy resources introduces a
   high variability that complicates the task of energy management in
   modern power grids. Among other technologies, batteries have proven
   effective in managing power imbalances in such grids. However, the high
   cost of large-scale batteries, coupled with their enormous space
   requirements, could deter their adoption by large consumers such as
   shared facility controllers. The aggregation of residential energy
   storage units offers shared facility controllers (SFCs) an alternative
   way to leverage storage; however, a secure scheme that promotes fairness
