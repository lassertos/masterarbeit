   Minist Agr, Key Lab Agriinformat, Beijing, Peoples R China.
   Beijing Engn Res Ctr Agr Internet Things, Beijing, Peoples R China.},
DOI = {10.1016/j.compag.2018.07.015},
ISSN = {0168-1699},
EISSN = {1872-7107},
Keywords = {Crop material information management; Genealogical tracking; Breeding
   informatization; Crop breeding information management system},
Keywords-Plus = {SYSTEM; TECHNOLOGIES},
Author-Email = {wangky@nercita.org.cn},
Times-Cited = {8},
