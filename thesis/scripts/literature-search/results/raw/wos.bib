   Qian, Kun, Peking Univ, Sch Stomatol, 22 Zhongguancun South St, Beijing 100081, Peoples R China.},
DOI = {10.1186/s12903-024-04565-3},
Article-Number = {827},
ISSN = {1472-6831},
Keywords = {Root canal therapy; Digital registration; CBCT; Scanned model;
