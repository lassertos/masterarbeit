{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ESNext"],
    "rootDir": "./src",
    "outDir": "./dist"
  },
  "exclude": ["./src-web"]
}
